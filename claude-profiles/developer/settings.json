{
	"$schema": "https://json.schemastore.org/claude-code-settings.json",
	"permissions": {
		"allow": [
			"Bash(*)",
			"Read(*)",
			"Write(*)",
			"Edit(*)",
			"Glob(*)",
			"Grep(*)",
			"WebFetch(*)",
			"WebSearch(*)",
			"NotebookEdit(*)",
			"mcp__*"
		],
		"deny": [
			"Bash(git push*)",
			"Bash(git push --force*)",
			"Bash(git push -f*)",
			"Bash(git reset --hard*)",
			"Bash(git clean -f*)",
			"Bash(rm -rf /*)",
			"Bash(rm -rf .*)",
			"Bash(: | *)"
		]
	}
}
