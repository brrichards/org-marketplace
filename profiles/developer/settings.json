{
  "extraKnownMarketplaces": {
    "org-marketplace": {
      "source": {
        "source": "github",
        "repo": "brrichards/org-marketplace"
      }
    }
  },
  "enabledPlugins": {
    "code-quality@org-marketplace": true,
    "git-workflow@org-marketplace": true,
    "testing@org-marketplace": true
  },
  "permissions": {
    "deny": [
      "Bash(git push --force*)",
      "Bash(git push * --force*)",
      "Bash(rm -rf /*)",
      "Bash(git reset --hard*)",
      "Bash(git checkout -- .)",
      "Bash(git clean -f*)"
    ]
  }
}
